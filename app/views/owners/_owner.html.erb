<tr>
    <td><%= link_to "#{owner.owner_lastname}#{owner.owner_firstname}", owner_path(owner) %></td>
    <td colspan='3'><%= JpPrefecture::Prefecture.find(owner.prefecture_code).name if owner.prefecture_code? %><%= owner.city %><%= owner.street %></td>
    <% if owner.phone_number1? %>
        <td><%= "#{owner.phone_number1}" + '-' + "#{owner.phone_number2}" + '-' + "#{owner.phone_number3}" %></td>
    <% else %>
        <td></td>
    <% end %>
    <td><%= link_to '削除', owner_path(owner), method: :delete %></td>
</tr>