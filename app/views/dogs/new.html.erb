<h1>Dogs#new</h1>
<p>Find me in app/views/dogs/new.html.erb</p>
<%= form_for @dog, html: { class: 'form-horizontal'} do |f| %>
    <div>
        <div>
            <img src="">画像</img>
        </div>
        <div>
            <button>画像登録</button>
        </div>
    </div>
    <div>
        <p>オーナー名：
        </p>
        <p>犬名：
            <%= f.text_field :dog_name %>
        </p>
        <p>犬種：
            <%= f.text_field :dog_breed %>
        </p>
        <p>犬生年月日：
            <%= f.date_field :dog_birthday %>
        </p>
        <p>食事タイプ：
            <%= f.select :food_type, { "ドッグフード" => "ドッグフード", "手作り食" => "手作り食", "生食" => "生食", "その他" => "その他" }, :include_blank => true %>
        </p>
        <p>食事回数／1日：
            <%= f.select :food_times, (1..9), :include_blank => true %>
        </p>
        <p>好きなおやつ：
            <%= f.text_field :favorite_snack1 %>
            <%= f.text_field :favorite_snack2 %>
            <%= f.text_field :favorite_snack3 %>
        </p>
        <p>好きなおもちゃ：
            <%= f.text_field :favorite_toy1 %>
            <%= f.text_field :favorite_toy2 %>
            <%= f.text_field :favorite_toy3 %>
        </p>
        <p>散歩回数／１日：
            <%= f.select :dogwalk_times, (1..9), :include_blank => true %> 回
        </p>
        <p>散歩時間／１回：
            <%= f.select :dogwalk_hours, (1..9), :include_blank => true %>  時間
        </p>
        <p>性格：
            <p>人への興味・反応
                <%= f.radio_button :character1, "A" %> A
                <%= f.radio_button :character1, "B" %> B
                <%= f.radio_button :character1, "C" %> C
            </p>
            <p>好奇心
                <%= f.radio_button :character2, "A" %> A
                <%= f.radio_button :character2, "B" %> B
                <%= f.radio_button :character2, "C" %> C
            </p>
            <p>服従心
                <%= f.radio_button :character3, "A" %> A
                <%= f.radio_button :character3, "B" %> B
                <%= f.radio_button :character3, "C" %> C
            </p>
            <p>人との協調性
                <%= f.radio_button :character4, "A" %> A
                <%= f.radio_button :character4, "B" %> B
                <%= f.radio_button :character4, "C" %> C
            </p>
            <p>人への愛情表現
                <%= f.radio_button :character5, "A" %> A
                <%= f.radio_button :character5, "B" %> B
                <%= f.radio_button :character5, "C" %> C
            </p>
            <p>適応能力
                <%= f.radio_button :character6, "A" %> A
                <%= f.radio_button :character6, "B" %> B
                <%= f.radio_button :character6, "C" %> C
            </p>
            <p>興奮性
                <%= f.radio_button :character7, "A" %> A
                <%= f.radio_button :character7, "B" %> B
                <%= f.radio_button :character7, "C" %> C
            </p>
            <p>集中力
                <%= f.radio_button :character8, "A" %> A
                <%= f.radio_button :character8, "B" %> B
                <%= f.radio_button :character8, "C" %> C
            </p>
        </p>
        <p>病歴・アレルギー：
            <p><%= f.text_field :clinical_history1 %></p>
            <p><%= f.text_field :clinical_history2 %></p>
            <p><%= f.text_field :clinical_history3 %></p>
            <p><%= f.text_field :clinical_history4 %></p>
            <p><%= f.text_field :clinical_history5 %></p>
        </p>
        <p>コマンド言語：
            <%= f.text_field :command_language %>
        </p>
        <p>基本コマンド：
            <p>おすわり
                <%= f.check_box :basic_command1, :as => :boolean %>
            </p>
            <p>ふせ
                <%= f.check_box :basic_command2, :as => :boolean %>
            </p>
            <p>まて
                <%= f.check_box :basic_command3, :as => :boolean %>
            </p>
            <p>おいで
                <%= f.check_box :basic_command4, :as => :boolean %>
            </p>
            <p>たって
                <%= f.check_box :basic_command5, :as => :boolean %>
            </p>
            <p>ついて
                <%= f.check_box :basic_command6, :as => :boolean %>
            </p>
        </p>
        <p>応用コマンド：
            <%= f.text_field :applied_command1 %>
            <%= f.text_field :applied_command2 %>
            <%= f.text_field :applied_command3 %>
            <%= f.text_field :applied_command4 %>
            <%= f.text_field :applied_command5 %>
        </p>
        <p>備考：
            <%= f.text_area :notes, :size => "60x10" %>
        </p>
        <%= f.submit "新規登録" %>
        <%= link_to "キャンセル", :back %>
    </div>

<% end %>