<h2>ドッグ一覧ページ</h2>
<% if @user.dogs.any? %>
<div class='row'>
    <div class='form-group'>
        <table class='table table-striped table-bordered table-hover'>
            <thead>
                <tr>
                    <th>犬名</th><th>犬種</th><th>犬年齢</th><th>削除</th>
                </tr>
            </thead>
            <tbody>
                <%= render @user.dogs %>
            </tbody>
        </table>
        <%= link_to 'ドッグ登録', new_dog_path, class: 'btn btn-primary' %>
    </div>
</div>
<% end %>
